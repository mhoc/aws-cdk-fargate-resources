{
    "compilerOptions": {
      "incremental": true,
      "inlineSourceMap": true,
      "lib": [
        "ES5",
        "ES2015",
        "ES2016",
        "ES2017",
        "ES2018",
        "ES2019",
        "ESNext.AsyncIterable"
      ],
      "module": "commonjs",
      "noUnusedLocals": true,
      "strict": true,
      "target": "es2017",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "jsx": "react",
      "jsxFactory": "createMjmlElement",
      "skipLibCheck": true
    },
    "exclude": ["node_modules"],
    "include": ["*.ts"]
  }
  